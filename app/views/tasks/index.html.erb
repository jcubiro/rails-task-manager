<div class="container">
<h1> TO DO's </h1>
<div class="container">
<% @tasks.each do |task| %>
  <div class="form-check">
    <label class="form-check-label" for="<%= task.title %>"><%= link_to task_path(task.id) do %><%= task.title%><% end %></label>
    <input class="form-check-input" type="checkbox" id="<%= task.title %>" name="exampleCheckbox1">
  </div>
<% end %>
</div>
</div>
